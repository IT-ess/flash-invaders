		"@sveltejs/kit": "^1.11.0",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"prisma": "^4.11.0",
		"svelte": "^3.56.0",
		"svelte-check": "^3.0.4",
		"sveltekit-i18n": "^2.2.2",
		"tailwindcss": "^3.2.7",
